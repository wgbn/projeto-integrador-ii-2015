<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>VaiParaClienteLista</from-outcome>
            <to-view-id>/ClienteLista.xhtml</to-view-id>
        </navigation-case> 
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/ClientLista.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>VoltarIndex</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>VaiParaIncluir</from-outcome>
            <to-view-id>/ClienteIncluir.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>VaiParaVisualizar</from-outcome>
            <to-view-id>/ClienteVisualizar.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>VaiParaeditar</from-outcome>
            <to-view-id>/ClienteEditar.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>refresh</from-outcome>
            <to-view-id>/ClienteLista.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/UsuarioLista.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltarIndex</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vaiParaIncluir</from-outcome>
            <to-view-id>/UsuarioIncluir.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>VaiParaeditar</from-outcome>
            <to-view-id>/UsuarioEditar.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>VaiParaVisualizar</from-outcome>
            <to-view-id>/UsuarioVisualizar.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>refresh</from-outcome>
            <to-view-id>/UsuarioLista.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/TipoAcaoLista.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltarIndex</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vaiParaIncluir</from-outcome>
            <to-view-id>/TipoAcaoIncluir.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>VaiParaeditar</from-outcome>
            <to-view-id>/TipoAcaoEditar.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>VaiParaVisualizar</from-outcome>
            <to-view-id>/TipoAcaoVisualizar.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>refresh</from-outcome>
            <to-view-id>/TipoAcaoLista.xhtml</to-view-id>
        </navigation-case>
</faces-config>
